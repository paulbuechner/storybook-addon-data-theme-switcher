{
  "name": "storybook-addon-data-theme-switcher",
  "version": "3.0.1",
  "description": "A Storybook addon to switch the data-theme attribute on the html element",
  "keywords": [
    "storybook-addon",
    "storybook-addon-theme",
    "storybook-addon-data-theme-switcher",
    "storybook-addon-theme-switcher",
    "storybook",
    "theme",
    "data-theme",
    "react",
    "tailwind",
    "tailwindcss",
    "daisyUI"
  ],
  "homepage": "https://github.com/paulbuechner/storybook-addon-data-theme-switcher",
  "repository": {
    "type": "git",
    "url": "https://github.com/paulbuechner/storybook-addon-data-theme-switcher"
  },
  "license": "MIT",
  "author": "Paul BÃ¼chner",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "default": "./dist/preview.js"
    },
    "./preset": "./dist/preset.js",
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "prebuild": "pnpm clean",
    "build": "tsup",
    "build-storybook": "storybook build",
    "build:watch": "pnpm build --watch",
    "changelog": "tsx scripts/changelog.ts",
    "changelog:commit": "git add . && git commit -am 'docs: add changelog' && git push",
    "changelog:format": "prettier --write .changelog",
    "changelog:latest": "tsx scripts/changelog.ts --latest",
    "clean": "rimraf ./dist",
    "gen:icon-names": "tsx scripts/generate-icon-names.ts",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky && pnpm run gen:icon-names",
    "release": "changeset publish",
    "start": "run-p build:watch \"storybook --quiet\"",
    "storybook": "storybook dev -p 6006",
    "test": "echo \"Error: no test specified\" && exit 1",
    "version": "changeset version"
  },
  "dependencies": {
    "@storybook/icons": "^2.0.1"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@changesets/get-release-plan": "^4.0.13",
    "@changesets/types": "^6.1.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@octokit/rest": "^22.0.1",
    "@storybook/addon-docs": ">=10.0.0-0 <11.0.0-0",
    "@storybook/react-vite": ">=10.0.0-0 <11.0.0-0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/edit-json-file": "^1.7.4",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.0.0",
    "@types/react": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "edit-json-file": "^1.8.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-storybook": ">=10.0.0-0 <11.0.0-0",
    "global": "^4.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "memoizerific": "^1.11.3",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "^6.1.0",
    "storybook": ">=10.0.0-0 <11.0.0-0",
    "tailwindcss": "^4.1.17",
    "tsup": "^8.5.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.4"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "storybook": "^10.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.25.0",
  "engines": {
    "node": ">=20.19.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "managerEntries": [
      "src/preset/manager.ts"
    ],
    "previewEntries": [
      "src/preset/preview.ts",
      "src/preset/index.ts"
    ],
    "nodeEntries": [
      "src/preset/index.ts",
      "src/preset/types.ts"
    ]
  },
  "storybook": {
    "displayName": "Storybook Addon Data Theme Switcher",
    "supportedFrameworks": [
      "react"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
