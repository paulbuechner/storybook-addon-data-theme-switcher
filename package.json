{
  "name": "storybook-addon-data-theme-switcher",
  "version": "2.0.0",
  "description": "A Storybook addon to switch the data-theme attribute on the html element",
  "keywords": [
    "storybook-addons",
    "storybook-addon-theme",
    "storybook-addon-data-theme-switcher",
    "storybook-addon-theme-switcher",
    "storybook",
    "theme",
    "data-theme",
    "react",
    "tailwind",
    "tailwindcss",
    "daisyUI"
  ],
  "homepage": "https://github.com/paulbuechner/storybook-addon-data-theme-switcher",
  "repository": {
    "type": "git",
    "url": "https://github.com/paulbuechner/storybook-addon-data-theme-switcher"
  },
  "license": "MIT",
  "author": "Paul BÃ¼chner",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./preset": "./dist/preset.cjs",
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "manager.js",
    "preview.js",
    "*.d.ts"
  ],
  "scripts": {
    "prebuild": "pnpm clean",
    "build": "tsup",
    "build-storybook": "storybook build",
    "build:watch": "pnpm build --watch",
    "changelog": "tsx scripts/changelog.ts",
    "changelog:commit": "git add . && git commit -am 'docs: add changelog' && git push",
    "changelog:format": "prettier --write .changelog",
    "changelog:latest": "tsx scripts/changelog.ts --latest",
    "clean": "rimraf ./dist",
    "gen:icon-names": "tsx scripts/generate-icon-names.ts",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky && pnpm run gen:icon-names",
    "release": "changeset publish",
    "start": "run-p build:watch \"storybook --quiet\"",
    "storybook": "storybook dev -p 6006",
    "test": "echo \"Error: no test specified\" && exit 1",
    "version": "changeset version"
  },
  "dependencies": {
    "@storybook/icons": "^1.4.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.4",
    "@changesets/get-release-plan": "^4.0.12",
    "@changesets/types": "^6.1.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.28.0",
    "@octokit/rest": "^22.0.0",
    "@storybook/addon-links": "^9.0.5",
    "@storybook/react-vite": "^9.0.5",
    "@tailwindcss/postcss": "^4.1.8",
    "@types/edit-json-file": "^1.7.3",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.1",
    "edit-json-file": "^1.8.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.0.5",
    "global": "^4.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "memoizerific": "^1.11.3",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.15",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^6.0.1",
    "storybook": "^9.0.5",
    "tailwindcss": "^4.1.8",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.3.5"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "storybook": "^9.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "exportEntries": [
      "src/preset/index.ts",
      "src/preset/types.ts"
    ],
    "managerEntries": [
      "src/preset/manager.ts"
    ],
    "previewEntries": [
      "src/preset/preview.ts"
    ]
  },
  "storybook": {
    "displayName": "Storybook Addon Data Theme Switcher",
    "supportedFrameworks": [
      "react"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
